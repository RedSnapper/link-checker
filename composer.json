{
    "name": "rs/link-check",
    "description": "My awesome package",
    "keywords": [
        "rs",
        "link-check"
    ],
    "homepage": "https://github.com/rs/link-check",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Param Dhaliwal",
            "email": "param@redsnapper.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "aws/aws-sdk-php": "^3.350",
        "voku/simple_html_dom": "^4.8"
    },
    "require-dev": {
        "laravel/pint": "^1.24",
        "orchestra/testbench": "^10.4",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "RedSnapper\\LinkChecker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RedSnapper\\LinkChecker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LinkCheckerServiceProvider"
            ],
            "aliases": {
                "LinkChecker": "RedSnapper\\LinkChecker\\Facades\\LinkChecker"
            }
        }
    }
}
